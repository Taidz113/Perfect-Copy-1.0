local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

-- Function to create a clone of the player
local function createClone()
    -- Error handling for character existence
    if not character then
        warn(DjCMTfJQfDdsUzdBSKwkxMhdtPRLpIXKHvGAuSlctymEpUdhmUvSQTjkXUPPmpYxUVXnhSWpdgHUqztGSCpRKdNLyoLovxXvcz('tCEXWzHDmTsbdtXftMUVVFYUcBhgbvstStnQNSRWdHDejIcuykSjbySQ2hhcmFjdGVyIG5vdCBmb3VuZCE='))
        return
    end

    -- Clone the character
    local clone = character:Clone()
    
    -- Set clone properties
    clone.Humanoid.Health = 99999  -- Immortal
    clone.Humanoid.MaxHealth = 99999
    clone.Humanoid.Damage = 1000    -- Attack power

    -- Set clone position to the playerDjCMTfJQfDdsUzdBSKwkxMhdtPRLpIXKHvGAuSlctymEpUdhmUvSQTjkXUPPmpYxUVXnhSWpdgHUqztGSCpRKdNLyoLovxXvcz('')s actions
    local function mimicActions()
        while true do
            if character and clone then
                -- Copy position and gestures
                clone:SetPrimaryPartCFrame(character.PrimaryPart.CFrame)
                clone.Humanoid:Move(character.Humanoid.MoveDirection)
                wait(0.1)  -- Adjust the wait time for smoother movement
            else
                break
            end
        end
    end

    -- Start mimicking actions
    pcall(mimicActions)
end

-- Execute the clone creation
pcall(createClone)
    
