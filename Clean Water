local b='ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/'
function tERURqqJlaCu(data) m=string.sub(data, 0, 60) data=data:gsub(m,'')

data = string.gsub(data, '[^'..b..'=]', '') return (data:gsub('.', function(x) if (x == '=') then return '' end local r,f='',(b:find(x)-1) for i=6,1,-1 do r=r..(f%2^i-f%2^(i-1)>0 and '1' or '0') end return r; end):gsub('%d%d%d?%d?%d?%d?%d?%d?', function(x) if (#x ~= 8) then return '' end local c=0 for i=1,8 do c=c+(x:sub(i,i)=='1' and 2^(8-i) or 0) end return string.char(c) end)) end


 


local function drainWater()
    -- Attempt to access the workspace
    local success, workspace = pcall(function()
        return game:GetService(tERURqqJlaCu('ojDiAceLAlzdwYhkfIkLRljBiSAbSqQygQpTjMnHpihudsvTJuarEHmjMWjnV29ya3NwYWNl'))
    end)

    -- Check if workspace was accessed successfully
    if not success or not workspace then
        error(tERURqqJlaCu('PFONVqoBoNYaYJkPhQiqvLjYwUhXlmiLubpLyquTNDZrQHAPRrajcbrNXKqSRmFpbGVkIHRvIGFjY2VzcyB0aGUgd29ya3NwYWNlLg=='))
    end

    -- Iterate through all parts in the workspace
    for _, object in pairs(workspace:GetChildren()) do
        -- Check if the object is a water part
        if object:IsA(tERURqqJlaCu('IlKVwLxplvEhdHpWecUSAVhONQzlDSJRPWYxZlWUtAVgYGFNdAGbIqpLuXGiUGFydA==')) and object.Material == Enum.Material.Water then
